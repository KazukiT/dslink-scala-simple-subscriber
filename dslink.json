{
  "name": "SimpleSubscriber",
  "version": "0.0.1",
  "description": "Subscribe a DSA node then print it on update",
  "license": "Cisco Sample Code License",
  "author": {
    "name": "Naoto Shimazaki",
    "email": "nshimaza@cisco.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nshimaza/SimpleSubscriberDSLink"
  },
  "bugs": {
    "url": "https://github.com/nshimaza/SimpleSubscriberDSLink/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "SimpleSubscriber"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "com.example.SimpleSubscriberDSLinkHandler"
    }
  }
}
